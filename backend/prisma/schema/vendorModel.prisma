model Vendor {
    id      Int    @id @default(autoincrement())
    name    String
    address String
    stateId Int?
    state   State? @relation(fields: [stateId], references: [id], onDelete: SetNull)
    userId  Int?   @unique
    user    User?  @relation(fields: [userId], references: [id], onDelete: SetNull)

    @@map("vendors")
}
