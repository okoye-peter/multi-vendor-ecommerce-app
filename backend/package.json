{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "src/Server.ts",
    "scripts": {
        "dev": "nodemon --watch src --exec ts-node src/Server.ts",
        "build": "tsc",
        "start": "node dist/Server.ts",
        "schema:combine": "node scripts/combine-schemas.mjs",
        "prisma:generate": "npm run schema:combine && prisma generate --schema=./prisma/generated-schema.prisma",
        "prisma:migrate": "npm run schema:combine && prisma migrate dev --schema=./prisma/generated-schema.prisma",
        "prisma:studio": "npm run schema:combine && prisma studio --schema=./prisma/generated-schema.prisma",
        "prisma:push": "npm run schema:combine && prisma db push --schema=./prisma/generated-schema.prisma",
        "prisma:seed": "ts-node prisma/seeders/index.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "dependencies": {
        "@bull-board/api": "^6.14.0",
        "@bull-board/express": "^6.14.0",
        "@prisma/adapter-pg": "^7.0.0",
        "bcryptjs": "^3.0.2",
        "bullmq": "^5.62.2",
        "cloudinary": "^2.8.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-useragent": "^2.0.2",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "nodemailer": "^7.0.9",
        "pg": "^8.16.3",
        "sharp": "^0.34.5",
        "winston": "^3.18.3",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@prisma/client": "^7.0.1",
        "@types/express": "^5.0.3",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.10.1",
        "@types/pg": "^8.15.6",
        "@types/sharp": "^0.31.1",
        "dotenv-cli": "^11.0.0",
        "nodemon": "^3.1.10",
        "prisma": "^7.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
